{
  "name": "swarm-city-api",
  "version": "0.1.0",
  "description": "API server for the Swarm City project.",
  "repository": {
    "type": "git",
    "url": "github:swarmcity/SwarmCityAPI"
  },
  "main": "src/socket.js",
  "bin": {
    "show-config": "./bin/show-config"
  },
  "scripts": {
    "start": "node src/server.js",
    "test": "nyc mocha --exit --timeout 180000 test/*",
    "test-unit": "nyc mocha --exit test/unit_*",
    "test-func": "nyc mocha --exit --timeout 180000 --grep \"(unit)\" --invert test/*",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint .",
    "esw": "esw .",
    "show-config": "node bin/show-config.js"
  },
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">= 7.6.0"
  },
  "dependencies": {
    "async": "^3.2.2",
    "bl": "^1.2.1",
    "crypto-js": "^3.1.9-1",
    "dotenv": "^4.0.0",
    "eslint-watch": "^3.1.3",
    "ethereumjs-util": "^5.1.3",
    "express": "^4.16.2",
    "ipfs-api": "git+https://github.com/sponnet/js-ipfs-api.git",
    "json-hash": "^1.2.0",
    "level": "^2.0.1",
    "request": "^2.83.0",
    "sinon": "^4.1.3",
    "socket.io": "^2.0.4",
    "uuid": "^3.1.0",
    "web3": "1.0.0-beta.35",
    "winston": "^3.3.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "eslint": "^4.10.0",
    "eslint-config-google": "^0.9.1",
    "mocha": "^4.0.1",
    "nyc": "^11.3.0",
    "should": "^13.1.3",
    "socketio-wildcard": "^2.0.0"
  }
}
